{
  "name": "redux.io",
  "version": "0.1.2",
  "description": "redux binding for socket.io",
  "main": "dist/redux.io.cjs.js",
  "module": "dist/redux.io.es.js",
  "browser": "dist/redux.io.umd.js",
  "scripts": {
    "prepare": "npm run build",
    "prepublish": "run-s build test",
    "prepublishOnly": "run-s build test",
    "build": "rollup -c",
    "build:app": "NODE_ENV=production rollup -c redux.io.messenger/rollup.config.js",
    "build:watch": "rollup -c -w",
    "clean": "rimraf dist",
    "dev": "run-p *:watch",
    "dev:server": "node redux.io.messenger/index.js",
    "test": "ava tests/redux.io.spec.js",
    "test:watch": "nodemon --exec 'npm test'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Francois-Esquire/redux.io.git"
  },
  "keywords": [
    "socket.io",
    "websocket",
    "react",
    "redux"
  ],
  "author": "Michael Tobia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Francois-Esquire/redux.io/issues"
  },
  "homepage": "https://github.com/Francois-Esquire/redux.io#readme",
  "peerDependencies": {
    "react": "^15.0.0-0 || ^16.0.0-0",
    "react-redux": "^4.3.0 || ^5.0.0",
    "redux": "^3.0.0",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "koa": "^2.3.0",
    "koa-router": "^7.2.1",
    "koa-send": "^4.1.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.1.1",
    "prop-types": "^15.5.10",
    "react": "^16.0.0-beta.5",
    "react-dom": "^15.6.1",
    "react-redux": "^4.3.0 || ^5.0.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.0.0",
    "redux-form": "^7.0.3",
    "rimraf": "^2.6.1",
    "rollup": "^0.49.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3"
  },
  "dependencies": {
    "hoist-non-react-statics": "^2.3.0"
  }
