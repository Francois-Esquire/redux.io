{
  "name": "redux.io",
  "version": "0.1.1",
  "description": "redux binding for socket.io",
  "main": "index.js",
  "scripts": {
    "prepublish": "run-s build test",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "run-p *:watch",
    "test": "ava tests/redux.io.spec.js",
    "test:watch": "nodemon --exec 'npm test'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Francois-Esquire/redux.io.git"
  },
  "keywords": [
    "socket.io",
    "websocket",
    "redux"
  ],
  "author": "Michael Tobia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Francois-Esquire/redux.io/issues"
  },
  "homepage": "https://github.com/Francois-Esquire/redux.io#readme",
  "dependencies": {
    "redux": "^3.7.1",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "buble": "^0.15.2",
    "buble-register": "^1.0.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "rollup": "^0.43.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.0.0",
    "socket.io-client": "^2.0.3"
  }
